# Environment Variables for Dormitory Management System
# Copy this to .env and modify with your actual values

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=dormitory_db
DB_USERNAME=root
DB_PASSWORD=

# Application Settings
APP_NAME="Dormitory Management System"
APP_URL=http://localhost/lasttry
APP_ENV=development
APP_DEBUG=true

# Admin Settings
ADMIN_EMAIL=admin@dormitory.com
ADMIN_NAME="System Administrator"

# File Upload Settings
UPLOAD_MAX_SIZE=5242880
UPLOAD_PATH=public/uploads/

# Session Settings
SESSION_LIFETIME=3600
SESSION_SECURE=false